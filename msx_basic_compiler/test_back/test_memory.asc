120 FORI=0TO1000:A$=STR$(1234)        :A=FRE(0):LOCATE0,1:PRINT A:NEXT
130 FORI=0TO1000:A$=LEFT$("123456",3) :A=FRE(0):LOCATE0,2:PRINT A:NEXT
140 FORI=0TO1000:A$=RIGHT$("123456",3):A=FRE(0):LOCATE0,3:PRINT A:NEXT
150 FORI=0TO1000:A$=SPACE$(10)        :A=FRE(0):LOCATE0,4:PRINT A:NEXT
