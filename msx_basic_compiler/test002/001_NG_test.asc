10 SCREEN 1:WIDTH32:CLS:KEY OFF
20 CLEAR 1000:DEFINT A-Z
30 DIM MP$(24)
50 FOR Y=0 TO 23:MP$(Y)=STRING$(32," ")
55 FOR X=0 TO 15
60 GOSUB 10000
70 NEXT X:NEXT Y
80 FOR I=0 TO 22:LOCATE 0,I:PRINT MP$(I);:NEXT I
85 LOCATE 0,23:PRINT LEFT$(MP$(23),31);
90 GOTO 90
10000 MID$(MP$(Y),X*2+1,2)=CHR$(X+65)+CHR$(Y+65):RETURN
